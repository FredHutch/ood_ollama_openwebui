# Wait for the open-webui to start
echo "Waiting for open-webui to open port ${webui_port}..."
# TODO shorten timeout
if wait_until_port_used "${host}:${webui_port}" 1200; then
  echo "Discovered open-webui listening on port ${webui_port}!"
else
  echo "Timed out waiting for open-webui to open port ${webui_port}!"
  clean_up 1
fi
sleep 2
